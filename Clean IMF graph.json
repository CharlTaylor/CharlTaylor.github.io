{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": {
    "text": "Outstanding credit of all IMF members",
    "subtitle": "Source: European parliment",
    "subtitleFontStyle": "italic",
    "subtitleFontSize": 10,
    "anchor": "start",
    "color": "black"
  },
  "data": {
    "url": "https://raw.githubusercontent.com/CharlTaylor/CharlTaylor.github.io/main/imf%20data%20clean.csv"
  },
  "height": 300,
  "width": 295,
  "mark": {"type": "bar", "color": "#86d0bb"},
  "encoding": {
    "x": {
      "field": "Date",
      "type": "temporal",
      "axis": {"title": null, "grid": false, "ticks": false, "labelAngle": 45}
    },
    "y": {
      "field": "as a %",
      "type": "quantitative",
      "title": "Outstanding debt as a proportion of GDP (out of 1)",
      "axis": {"grid": false}
    }
  },
  "config": {}
}